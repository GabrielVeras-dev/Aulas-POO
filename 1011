import java.util.Scanner;

public class Esfera {
	public static void main(String[] args) {

	Scanner ler = new Scanner (System.in);
	
	double esfera, pi, raio; 

	pi=3.14159;

	System.out.println("Escreva o Raio:");
	raio = ler.nextDouble();

	esfera = (4/3.0) pi Math.pow(raio,3);

	 System.out.println("Volume = " + esfera);
	}
}
